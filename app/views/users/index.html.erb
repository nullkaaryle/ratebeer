<p id="notice"><%= notice %></p>

<h1>Users</h1>

<p> Ratebeer has <%= pluralize(@users.count, 'user') %>: </p>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= link_to 'Show user profile', user %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
